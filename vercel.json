{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/employee",
      "destination": "/employee.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/management",
      "destination": "/management.html"
    },
    {
      "source": "/",
      "destination": "/home.html"
    },
    {
      "source": "/(.*)",
      "destination": "/home.html"
    }
  ]
}
