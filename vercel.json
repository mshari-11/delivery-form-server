{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|pdf))",
      "dest": "/public/$1"
    },
    {
      "src": "/form-complete.html",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard.html",
      "dest": "/api/index.js"
    },
    {
      "src": "/login.html",
      "dest": "/api/index.js"
    },
    {
      "src": "/index.html",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.js"
    },
    {
      "src": "/login",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}